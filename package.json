{
  "name": "fitness-tracker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "tailwindcss": "3.0.0",
    "firebase": "9.6.1",
    "express": "4.17.1",
    "mongoose": "6.2.0",
    "zustand": "3.6.0",
    "sentry": "6.13.0",
    "dotenv": "10.0.0"
  },
  "devDependencies": {
    "@types/react": "18.0.0",
    "@types/node": "17.0.0",
    "jest": "^27.0.0",
    "vite": "^2.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": ["/node_modules/", "/build/"],
    "collectCoverage": true,
    "coverageReporters": ["json", "lcov", "text"],
    "setupFilesAfterEnv": ["<rootDir>/src/setupTests.js"],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "clearMocks": true,
    "verbose": true
  }
}